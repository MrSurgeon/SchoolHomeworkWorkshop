@model AdminRoleAddModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>RoleEdit</title>
</head>
<body>
    <form asp-controller="Admin" asp-action="RoleEdit">
        <div>
            <label>Id:</label>@Model.Role.Id
            <input type="hidden" asp-for="Role.Id"/>
        </div>
        <div>
            <label>RoleName:</label>
            <input type="text" asp-for="Role.Name"/>
            <input type="hidden" asp-for="Role.ConcurrencyStamp"/>
        </div>
        <input type="submit" value="Update" />
    </form>
</body>
</html>
