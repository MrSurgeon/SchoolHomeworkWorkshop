@model MyCompany.School.HomeworkDemo.Areas.AdminAuthorities.Models.BranchModels.BranchManagerModel 
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Edit</title>
</head>
<body>
    <form asp-route-area="AdminAuthorities" asp-controller="Branch" asp-action="Edit" method="post">
        <div asp-validation-summary="All"></div>
        <div>
            <input type="hidden" asp-for="Branch.Id" />
            <label asp-for="Branch.BranchName"></label>
            <input type="text" asp-for="Branch.BranchName"/>
        </div>
        <br />
        <div>
            <input type="submit" value="Submit" />
        </div>
    </form>
</body>
</html>
